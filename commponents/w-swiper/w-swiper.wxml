<!--commponents/w-swiper/w-swiper.wxml-->
<swiper class="swiper1" autoplay circular interval="3000" indicator-dots indicator-active-color="#ff5777" >
   <block wx:for="{{list}}" wx:key='index'>
     <swiper-item >
     <image src="{{item}}" mode="widthFix" class="swiper-item"></image>
     </swiper-item>
   </block>
</swiper>
